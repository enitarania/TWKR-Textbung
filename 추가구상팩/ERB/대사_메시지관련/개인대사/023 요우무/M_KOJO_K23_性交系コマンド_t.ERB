	;시간중
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;연모
	ELSEIF TALENT:연모
		PRINTFORMW 
		RETURN 1
	;굴복 3
	ELSEIF MARK:괘씸각인 == 3
		PRINTFORMW 
		RETURN 1
	;그 이외
	ELSE
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF
