	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;연모
	ELSEIF TALENT:연모
		PRINTFORMW 「아앗, 아파♪」
            	PRINTFORMW 「으우…… 엉덩이가 얼얼해요……」
		RETURN 1
	;굴복 3
	ELSEIF MARK:괘씸각인 == 3
		PRINTFORMW 「앗, 크읏, 하아앗♪」
            	PRINTFORMW 「요우무에게 좀 더 벌을 주세요, %CALLNAME:MASTER%♪」
		RETURN 1
	;그 이외
	ELSE
		PRINTFORMW 「크읏…… 응읏…… 아파♪」
            	PRINTFORMW 「앗, 지, 지금 건……」
		RETURN 1
	ENDIF
ENDIF
RETURN 1

