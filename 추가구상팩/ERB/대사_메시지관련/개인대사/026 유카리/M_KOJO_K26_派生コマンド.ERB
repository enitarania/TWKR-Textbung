;1,シックスナイン
;==================================================
@M_KOJO_MESSAGE_SCOM_K26_1
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_SCOM_K26_1_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K26_1_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		PRINTFORMW 「응……당신도 하면서？」
		PRINTFORMW 「좋아。그치만、상냥하게 부탁해」
		PRINTFORMW 「……너무 격렬하게하면、이쪽이 집중할 수 없으니까」
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;恋慕
	ELSEIF TALENT:연모
		IF RAND:2 == 0
			PRINTFORMW 「읏、힉、으응……」
			PRINTFORMW 때때로 사랑스러운 소리를 내는 유카리를、%조사처리(CALLNAME:MASTER,"는")% 부드럽게 혀로 핥아줬다
		ELSE
			PRINTFORML 「하응, 읏크……기분 좋아, 핫, 그렇게 해 버리면……」
			PRINTFORMW 「응, 읏, 내가 집중할 수가 없게 돼 버리잖아……읏」
		RETURN 1
		ENDIF
	;屈服3
	ELSEIF MARK:괘씸각인 == 3
		PRINTFORMW 「하앗……응……서로 사랑해주는 거、좋은걸……」
		RETURN 1
	;それ以外
	ELSE
		PRINTFORMW 「읏、으읏、우우……히아앗！」
		PRINTFORMW %조사처리(CALLNAME:MASTER,"가")% 혀를 움직일때마다 유카리는 충격을 받은 듯 몸을 떨었다
		RETURN 1
	ENDIF
ENDIF
;未記入
;CALLFORM M_KOJO_MESSAGE_COM_K26_{SELECTCOM}_1
RETURN 1

;==================================================
;2,岩清水
;==================================================
@M_KOJO_MESSAGE_SCOM_K26_2
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_SCOM_K26_2_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K26_2_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		PRINTFORMW 「하웃！　시럿、소리라니！」
		PRINTFORMW 「아읏、아아앗！」
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;恋慕
	ELSEIF TALENT:연모
	    IF ABL:노출벽 >= 10 && ABL:0 >= 10
	        IF RAND:2 == 0
                 PRINTFORMW 「이런 모습… 부… 부끄러워…. 하… 하지만 기분이 너무 좋아서…」
                 PRINTFORMW 「%CALLNAME:MASTER%…. 봐…! 나의 이 부끄러운 모습을 봐줘…!」
            ELSE
                 PRINTFORMW 「당신을 아주 가까이에서 느낄 수 있어…. ―나의 부끄러운 모습…좀 더 봐줘…」
            ENDIF
        ELSE
		    IF RAND:2 == 0
			    PRINTFORMW 「아읏！　몸속까지、느껴버려어엇！」
			    PRINTFORMW 새빨갛게 물든 얼굴을 손으로 가리면서、유카리는 뜨거워진 애액을 터트렸다
		    ELSE
			    PRINTFORMW 「응、응으응……햐앙、이건 좀……부끄러워…………」
			    PRINTFORMW 「그냥 핥기만 해도 부끄러운데…。이런 모습은 안하면 안될까…？」
			ENDIF
		ENDIF
		RETURN 1
	;屈服3
	ELSEIF MARK:괘씸각인 == 3
		PRINTFORMW 「읏、숨 쉬어져……？」
		PRINTFORMW 「허리가 빠져버렸어、미안해에」
		RETURN 1
	;それ以外
	ELSE
		PRINTFORMW 「으으읏…… 이런 굴욕을…… 후회할거야……」
		RETURN 1
	ENDIF
ENDIF
