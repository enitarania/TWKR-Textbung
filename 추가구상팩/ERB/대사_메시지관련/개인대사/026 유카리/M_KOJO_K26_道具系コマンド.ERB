;40,ローター
;==================================================
@M_KOJO_MESSAGE_COM_K26_40
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K26_40_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K26_40_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 「큿、아아앗！　그런、꽉 눌러지면！」
		PRINTFORMW 「앗、좋、좋아아앗！기분조하아아앗！」
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;恋慕
	ELSEIF TALENT:연모
		PRINTFORMW 「아앗、이런 장난감으로、느껴버리다니이잇！」
		PRINTFORMW 「하읏、하아、아아앗！」
		RETURN 1
	;屈服3
	ELSEIF MARK:괘씸각인 == 3
		PRINTFORMW 「핫 하아아악! ……좋아앗…… 기분 좋아…………」
            	PRINTFORMW 「그… 그이마아안. %조사처리(CALLNAME:MASTER,"가")% 해 주는 쪽이… 더 기뻐어어엇…!」
		RETURN 1
	;それ以外
	ELSE
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF
RETURN 1

