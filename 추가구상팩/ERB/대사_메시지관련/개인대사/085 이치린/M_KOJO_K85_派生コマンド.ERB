		SELECTCASE RAND:2
		CASE 0
			PRINTFORML 「째%UNICODE(0x2764)%오쿠쯔 쉬면%UNICODE(0x2764)%」
			PRINTFORML 「간히 좋은%UNICODE(0x2764)%다 히라고%UNICODE(0x2764)%고향%UNICODE(0x2764)%」
		CASE 1
			PRINTFORML 「응″%UNICODE(0x2764)%%UNICODE(0x2764)%두는 톡톡%UNICODE(0x2764)%라고 히%UNICODE(0x2764)%」
		ENDSELECT
	ELSEIF TALENT:78
		SELECTCASE RAND:3
		CASE 0
			PRINTFORML 「두는 %UNICODE(0x2764)%%UNICODE(0x2764)%-사랑이라고 끝내고 있는 %UNICODE(0x2764)%」
			PRINTFORML 「%CALLNAME:MASTER%의 좀 더 두는 것으로 사정해 %UNICODE(0x2764)%%UNICODE(0x2764)%」
		CASE 1
			PRINTFORML 「응등%UNICODE(0x2764)%안돼%UNICODE(0x2764)%둘 때까지 물어 버리고 있는 %UNICODE(0x2764)%%UNICODE(0x2764)%」
			PRINTFORML 「둘 때까지 사랑받고 있는 %UNICODE(0x2764)% 좀 더 깊고 %UNICODE(0x2764)%%UNICODE(0x2764)%」
		CASE 2
			PRINTFORML 「깊은 %UNICODE(0x2764)%후미질 수 있는 괴롭힐 수 있는 것 수기 있고%UNICODE(0x2764)%%UNICODE(0x2764)%」
			PRINTFORML 「″″%UNICODE(0x2764)%%UNICODE(0x2764)%」
		ENDSELECT
	ELSE
		SELECTCASE RAND:2
		CASE 0
			PRINTFORML 「아아앙%UNICODE(0x2764)%그런깊은 %UNICODE(0x2764)%」
		CASE 1
			PRINTFORML 「둔다%UNICODE(0x2764)% 좀 더 %UNICODE(0x2764)%좋은 %UNICODE(0x2764)%비어 있고%UNICODE(0x2764)%%UNICODE(0x2764)%」
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF
