;==================================================
;6, 삽입 G스포 고문
;==================================================
@M_KOJO_MESSAGE_SCOM_K85_6
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_SCOM_K85_6_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K85_6_1
;-------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 0
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;처음
	;기입 체크
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && TCVAR:101 == PLAYER
		;전회 사정했다
		IF TCVAR:104
			PRINTFORML 「지금%UNICODE(0x2764)%사정했던 바로 직후인데 %UNICODE(0x2764)%」
			PRINTFORML 「거기좋은 %UNICODE(0x2764)%안돼%UNICODE(0x2764)%이지메등째%UNICODE(0x2764)%%UNICODE(0x2764)%」
			RETURN 1
		ELSE
			PRINTFORMW 
			RETURN 1
		ENDIF
	ENDIF
	;기본 세트
	IF TCVAR:140
		PRINTFORML 「%UNICODE(0x2764)%거기 수기 있고%UNICODE(0x2764)%있고 공주라고 %UNICODE(0x2764)%%UNICODE(0x2764)%」
	ELSE
		SELECTCASE RAND:2
		CASE 0
			PRINTFORML 「응%UNICODE(0x2764)%거기는 약하기 때문에%UNICODE(0x2764)%안돼%UNICODE(0x2764)%」
		CASE 1
			PRINTFORML 「기분 사촌뿐 %UNICODE(0x2764)%이상해져 버리는%UNICODE(0x2764)%%UNICODE(0x2764)%」
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF
;미기입
;CALLFORM M_KOJO_MESSAGE_COM_K85_{SELECTCOM}_1
RETURN 1

