	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM) && !FLAG:70
		;처녀 분기
		;IF TCVAR:파과 == 1
			;IF TALENT:연인
				;PRINTFORML 
			;ELSEIF TALENT:연모
				;PRINTFORML 
			;ELSEIF TALENT:사모
				;PRINTFORML 
			;ELSE
				;PRINTFORML 
			;ENDIF
			;RETURN 1
		;ELSE
	IF TALENT:연인
		PRINTFORML 
