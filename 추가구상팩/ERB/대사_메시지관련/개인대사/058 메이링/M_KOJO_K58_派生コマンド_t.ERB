;1, 식스나인
;==================================================
@M_KOJO_MESSAGE_SCOM_K58_1
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_SCOM_K58_1_1
RETURN RESULT
@M_KOJO_MESSAGE_SCOM_K58_1_1
;-------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;처음
	;기입 체크
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		PRINTFORMW 「이 체위, 참 야릇해져요…」
		PRINTFORMW 「기분 좋아져요♪」
		RETURN 1
	ENDIF
	;기본 세트
	;시간중
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;연모
	ELSEIF TALENT:연모
		PRINTFORMW 「햐앙! 거기, 안 돼요, 기닷… 히아앗!!」
		PRINTFORMW 쾌락에 휘둘리는 메이링의 손과 입의 움직임이 굳어진다.
		RETURN 1
	;굴복 3
	ELSEIF MARK:괘씸각인 == 3
		PRINTFORMW 「쮸팟… 쮸, 응… 거기… 좋아요…」
		RETURN 1
	;그 이외
	ELSE
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF
;미기입
;CALLFORM M_KOJO_MESSAGE_COM_K58_{SELECTCOM}_1
RETURN 1

