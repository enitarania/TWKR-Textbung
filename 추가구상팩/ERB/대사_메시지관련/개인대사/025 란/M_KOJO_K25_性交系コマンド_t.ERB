	IF LOCAL:1 && TCVAR:100 == PLAYER
		;지금 사정했다
		IF NOWEX:PLAYER:11
			;시간중
			IF FLAG:70
				SELECTCASE RAND:3
				CASE 0
					PRINTFORML 사정의 흥분조차 식지 않는 사이, 짐승의 교미 자세로 페니스를 박았다
					PRINTFORML 저항도 못한 채 더럽혀진 질 구멍이 본능적으로 꿀렁꿀렁 정액을 뽑아내는 직임을 반복하고 있다
				CASE 1
					PRINTFORML %마스터는% 문득, 개들이 할 만한 교미 자세에 도전했다
					PRINTFORML 삽입한 채로 몸을 반대로 회전시켜, 어떻게든 엎드려 엉덩이끼리 맞추는 것에 성공한다
					PRINTFORML 허리를 들어 올린 아크로바틱한 자세로 란의 자궁구를 찔러주었다
				CASE 2
					PRINTFORML 시간이 멈춘 란의 엉덩이에, 배 안쪽을 귀두로 후려갈기는 것만 같은 피스톤질을 한다
					PRINTFORML 느끼는 것이 아픔이든 쾌감이든, 그녀의 질벽은 페니스를 더 원하는 듯 쮸읍쮸읍 음란한 소리를 냈다
				ENDSELECT
				RETURN 1
			ELSE
				SELECTCASE RAND:3
				CASE 0
					PRINTFORML 배로부터 북받쳐오는 열기에 몸을 맡겨, 란의 등 뒤에서 잡은 엉덩이를 격렬하게 푹푹 범한다
					PRINTFORML 「응, 앗, 아핫, 앗, 아핫……!」
					PRINTFORML 개처럼 엎드린 자세로 받아들이는 란의 질내에, 피스톤을 멈추지 않고 계속 정액을 부어넣는다
				CASE 1
					PRINTFORML 쿨룩, 쿨룩, 쿨룩……
					PRINTFORML 일단 페니스를 뽑아내고, 지금까지 괴롭혀 준 란의 보지를 바라본다
					PRINTFORML 숨을 헐떡이며 푹 엎드린 란의 거기로부터 새하얀 점액이 흘러내렸다
				CASE 2
					PRINTFORML 사정해버려도 %마스터는% 허리를 멈추지 않는다
					PRINTFORML 강하게 엉덩이를 칠 때마다, %CALLNAME:MASTER%의 배 밑에 엎드린 란의 거유가 바르르 떨렸다
				ENDSELECT
				RETURN 1
			ENDIF
		ELSE
			;시간중
			IF FLAG:70
				PRINTFORML 몸의 자세를 바꾸어 뜨겁고 부드러운 구멍을 페니스로 계속 범했다
				RETURN 1
			ELSE
				PRINTFORML 허리를 멈추지 않고 격렬한 삽입을 이어나간다
				PRINTFORML 뜨겁고 달아오른 질내를 쑤실때마다 란이 소리를 높여만 간다
				RETURN 1
			ENDIF
		ENDIF
	ENDIF
