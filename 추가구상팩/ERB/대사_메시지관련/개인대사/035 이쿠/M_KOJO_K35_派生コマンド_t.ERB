		ELSE
			PRINTFORMW 
			RETURN 1
		ENDIF
	ENDIF
	;基本セット
	IF TCVAR:140
		PRINTFORML 「햐읏%UNICODE(0x2764)% 거기 조아%UNICODE(0x2764)% 더 게롭혀져어%UNICODE(0x2764)%%UNICODE(0x2764)%」
	ELSE
		SELECTCASE RAND:2
		CASE 0
			PRINTFORML 「으응%UNICODE(0x2764)% 거긴 약한 곳이라%UNICODE(0x2764)% 안 돼%UNICODE(0x2764)%」
		CASE 1
			PRINTFORML 「기분 좋은 것만 잔뜨윽%UNICODE(0x2764)% 이상해져버려%UNICODE(0x2764)%%UNICODE(0x2764)%」
		ENDSELECT
		RETURN 1
	ENDIF
