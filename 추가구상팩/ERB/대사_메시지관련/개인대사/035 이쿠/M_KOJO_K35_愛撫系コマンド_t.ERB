@M_KOJO_MESSAGE_COM_k35_0_1
;-------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;처음
	;기입 체크
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORML  입을 맞추고, 허리를 안은 채 등을 손가락으로 쓰다듬는다
		PRINTFORML  조심스레 침대로 밀어 넘어뜨려 가슴을 만지자
		PRINTFORMW  크고 부드러운 가슴이 쥘 때마다 조금씩 그 모습을 바꿔 간다
		PRINTFORML  잠시 그 감촉을 즐기고 있자 관능적인 한숨이 흘러 나온다
		PRINTFORMW  천천히 한 손을 가슴에서 배꼽으로, 허리로 쓰다듬으며 음부까지 내려간다
		PRINTFORML  조금 습기를 머금은 속옷 위로 살살 만지며 자극한다
		PRINTFORML  천천히 애태우듯 힘을 조절하며 전신을 애무해 몸을 적응시켜 간다
		PRINTFORMW 「츄읍……%UNICODE(0x2764)%」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;삽입중
	;기입 체크
	LOCAL:1 = 1
	;-------------------------------------------------
	;V삽입
	IF LOCAL:1 && TEQUIP:50 == PLAYER
		;사정했다
		IF NOWEX:PLAYER:11
			IF TCVAR:140
				PRINTFORML 「하아……%UNICODE(0x2764)% 이거 굉장해%UNICODE(0x2764)%%UNICODE(0x2764)%」
				PRINTFORML 「더어……%UNICODE(0x2764)% 안까지…… 범해줘%UNICODE(0x2764)%%UNICODE(0x2764)%」
			ELSE
				SELECTCASE RAND:4
				CASE 0
					PRINTFORML 「으응……%UNICODE(0x2764)% 더…… 더 찔러줘%UNICODE(0x2764)%」
					PRINTFORMW 「사정해 줘%UNICODE(0x2764)% 그대로 들어와 줘%UNICODE(0x2764)%」
				CASE 1
					PRINTFORML 「아앙……%UNICODE(0x2764)% 그렇게 한 번에……%UNICODE(0x2764)%」
					PRINTFORMW 「가버릴 것 같나요? 좋아요%UNICODE(0x2764)% 그대로 가 주세요%UNICODE(0x2764)%」
				CASE 2
					PRINTFORML 「으응……%UNICODE(0x2764)% 격렬……해엣%UNICODE(0x2764)%」
					PRINTFORMW 「움찔움찔하고 있어%UNICODE(0x2764)% 그대로 뽑지 말아줘%UNICODE(0x2764)%」
				CASE 3
					PRINTFORML 「안 돼%UNICODE(0x2764)% 그런%UNICODE(0x2764)%」
					PRINTFORMW 「전부 사랑해줘%UNICODE(0x2764)% 이상해져 버려%UNICODE(0x2764)%%UNICODE(0x2764)%」
				ENDSELECT
			ENDIF
			RETURN 1
		ELSE
			IF TCVAR:140
				PRINTFORML 「하아……%UNICODE(0x2764)% 이거 굉장해%UNICODE(0x2764)%%UNICODE(0x2764)%」
			ELSE
				SELECTCASE RAND:4
				CASE 0
					PRINTFORML 「으응……%UNICODE(0x2764)% 더…… 더 찔러줘%UNICODE(0x2764)%」
				CASE 1
					PRINTFORML 「아앙……%UNICODE(0x2764)% 그렇게 한 번에……%UNICODE(0x2764)%」
				CASE 2
					PRINTFORML 「으응……%UNICODE(0x2764)% 격렬……해엣%UNICODE(0x2764)%」
				CASE 3
					PRINTFORML 「안 돼%UNICODE(0x2764)% 그런%UNICODE(0x2764)%」
				ENDSELECT
			ENDIF
			RETURN 1
		ENDIF
	;A삽입
	ELSEIF LOCAL:1 && TEQUIP:51 == PLAYER
		;사정했다
		IF NOWEX:PLAYER:11
			PRINTFORMW 「으응……%UNICODE(0x2764)% 더…… 더 찔러줘%UNICODE(0x2764)%」
			PRINTFORMW 「사정해 줘%UNICODE(0x2764)% 그대로 들어와 줘%UNICODE(0x2764)%」
			RETURN 1
		ELSE
			PRINTFORMW 「으응……%UNICODE(0x2764)% 더…… 더 찔러줘%UNICODE(0x2764)%」
			RETURN 1
		ENDIF
	ENDIF
	;-------------------------------------------------
	;후타나리
	;기입 체크
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && (TALENT:2 & 3) == 3
		PRINTFORMW 
		RETURN 1
	ENDIF
	;기본 세트
	;시간중
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;연모
	ELSEIF TCVAR:140
			PRINTFORML 「하아……%UNICODE(0x2764)% 애태우지만 말고%UNICODE(0x2764)%%UNICODE(0x2764)%」
			PRINTFORMW 「빨리…… 빨리 넣어 주세요%UNICODE(0x2764)%%UNICODE(0x2764)%」
	ELSEIF TALENT:연모
		SELECTCASE RAND:3
		CASE 0
			PRINTFORML 「아앙%UNICODE(0x2764)% 상냥하시네요……%UNICODE(0x2764)%」
			PRINTFORMW 「츄우읏%UNICODE(0x2764)% 더 격렬하게 하셔도 상관없어요%UNICODE(0x2764)%」
		CASE 1
			PRINTFORML 「아앙%UNICODE(0x2764)% 하아하아…… 애태우기만 하고%UNICODE(0x2764)%」
			PRINTFORML 「괴롭히지 말고 빨리 가게 해주세요%UNICODE(0x2764)%
			IF TALENT:연인
				PRINTFORML 서방님%UNICODE(0x2764)%
			ENDIF
			PRINTW 」
		CASE 2
			PRINTFORML 「츄읏%UNICODE(0x2764)% 후훗…… 사양마시고」
			PRINTFORMW 「더…… 안아주셔도 상관없으니까요%UNICODE(0x2764)%」
		ENDSELECT
	;굴복 3
	ELSEIF MARK:괘씸각인 == 3
		PRINTFORMW 
		RETURN 1
	;그 이외
	ELSE
		SELECTCASE RAND:2
		CASE 0
			PRINTFORMW 「읏……키, 키스도……좋습니까?%UNICODE(0x2764)%」
		CASE 1
			PRINTFORMW 「아읏……가슴만……그, 그쪽도……부탁 합니다……%UNICODE(0x2764)%」
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF
RETURN 1
