;-------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF CFLAG:317 == 1
		IF TALENT:연인 && TALENT:연모
			PRINTFORML 「…………」
			PRINTFORML 「…………………………」
			PRINTFORML 「……………………………………………………」
			PRINTFORMW 「정말이지……당신은 사람은……」
			PRINTFORML 「서로 사랑을 서로 확인해 , 보다 한층 서로 이해하기 때문에(위해) 의 행위의 한창때입니다만」
			PRINTFORML 「·에·을·해·이라고·있고··응·으로··인가?」
			PRINTFORMW 카센의 오른 팔로 어깨를 잡아져%마스터는% 당황해 카센 쪽을 다시 향했다 ……
		ELSEIF TALENT:연모
			PRINTFORML 「…………」
			PRINTFORML 「…………………………」
			PRINTFORML 「그런가요%CALLNAME:MASTER%로 초밥」
			PRINTFORMW 「나에게 안는 감정은 굉장한 물건은 아니군요」
			PRINTFORML 「에에, 부디, 그대로 청소를 계속해 주세요」
			PRINTFORMW 카센에 따져져%마스터는% 당황해 카센 쪽을 다시 향했다 ……
		ELSEIF TALENT:사모
			PRINTFORML 「…………」
			PRINTFORML 「아, 나에게 상관하지 않고 계속해 주셔 좋아요?」
			PRINTFORML 「거처를 깨끗이 해 두는 일은 매우 소중하기 때문에」
			PRINTFORMW 카센의 소리로부터 분명한 노기가 배어 나오고 있는……%마스터는% 당황해 카센 쪽을 다시 향했다 ……
		ENDIF
	ELSE
		IF TALENT:연모
			SELECTCASE RAND:5
			CASE 0
				PRINTFORML 「혼자서는 대단하겠지? 나도 도와요」
				PRINTFORMW 「레이무는 곧 먼지를 모으고……」
			CASE 1
				PRINTFORML 「변함없이 더러워져」
				PRINTFORMW 「다시 수행이 필요할까……에서도……으응~」
			CASE 2
				PRINTFORML 「거처가 더러워져 있으면(자), 거기에 살고 있는 사람의 마음도 더러워집니다」
				PRINTFORMW 「……그렇지만, 이 넓이를 매일 전부 청소하는 것은 분명히 무리예요 응」
			CASE 3
				SELECTCASE DAY:2
				CASE 1
					PRINTFORML 「밖의 세계에서는, 봄에 대청소를 하는 지역이 있다고 합니다」
					PRINTFORMW 「이라고 말하는 것으로, 확실히 청소해요!」
				CASE 2
					PRINTFORML 「후우……분명히 덥네요」
					PRINTFORML 카센은 촉촉하게 땀을 흘리고 있다
					PRINTFORMW %CALLNAME:MASTER%의 시선은 카센의 목덜미에 정부(제대로 고정) 가 되어 버렸다……
				CASE 3
					PRINTFORML 「등의 가을라는 말이 많습니다만, 저것은 가을의 날씨가 보내기 쉽기 때문에입니다」
					PRINTFORMW 「이라고 말하는 것으로, 확실히 청소해요!」
				CASE 4
					SELECTCASE DAY:3
					CASE 28
						PRINTFORML 「일년의 마지막의 대청소, 손은 빠지지 않습니다」
						PRINTFORMW 「……오늘은 진심을 보입니다」
						PRINTFORML 카센은 굉장한 속도로 움직이면서 청소를 하고 있는……이제 사람의 재주는 아니다
						PRINTFORMW 「이봐 %CALLNAME:MASTER%!손을 끊고 있는 짬은 없어요!」
					CASEELSE
						PRINTFORML 「우우……수가 차갑다……」
						PRINTFORML 「어? , 뭐? 어떻게 된거야 %CALLNAME:MASTER%?」
						PRINTFORMW %마스터는% 냉수에 노출된 카센의 손을 잡아, 양손으로 감싸 따뜻하게 했다
						PRINTFORML 「아……수줍어요」
						PRINTFORMW 「, 이봐, 이런 금년이라고 없고 청소하지 않으면!」
					ENDSELECT
				ENDSELECT
			CASE 4
				SELECTCASE CFLAG:300
				CASE 1,2,3,5,11,20,22,25,36
					SELECTCASE DAY:2
					CASE 1
						PRINTFORML 「우우-응, 좋은 날씨예요」
						PRINTFORMW 「청소의 해가 겉껍데기 있다고 하는 것」
					CASE 2
						PRINTFORML 「이렇게 더우면 분명히 옥외의청소는 피하고 싶네요」
						PRINTFORMW 「물을 뿌림을 해 더위를 완화해 둡시다」
					CASE 3
						PRINTFORML 「%CALLNAME:MASTER%, 군고구마라도 만들지 않아?」
						PRINTFORML 「쓰레기도 재이용할 수 있다면, 그렇게 해야 하는 것」
						PRINTFORMW 「케케켓 해 식욕의 가을이라고 그런 이유는 없어, 에에에」
					CASE 4
						PRINTFORML 「눈치우기는 너무 자신있지 않아」
						PRINTFORMW 「내 집에는, 눈은 쌓이지 않게 되어 있을테니까」
					ENDSELECT
				CASE 14,40
					PRINTFORML 「더럽기 때문에라고 해 기죽음 해서는 안 돼요」
					PRINTFORMW 「누군가가 청소하지 않으면, 더러워질 뿐이니까요」
				CASE 12,23
					SELECTCASE RAND:10
					CASE 0
						PRINTFORML 「속옷을 벗어 던져 두다니……」
						PRINTFORMW 「봐에 오지 않아 좋습니다!그쪽을 청소해 주세요!」
					CASEELSE
						PRINTFORML 「이런 곳은 곰팡이가 나기 쉬워요」
						PRINTFORMW 「청소의 뒤는 확실히 환기, 잊지 말아 주세요」
					ENDSELECT
				CASE 13,24,39
					PRINTFORML 「……응%CALLNAME:MASTER%」
					PRINTFORMW 「모처럼 옷을 벗었다 해, 함께 편히 쉬지 않습니까?」
				CASE 8,29,41,42
					PRINTFORML 「이런 곳은 정성스럽게 청소하지 않으면」
					PRINTFORMW 「나와, 아레가」
				CASE 19
					PRINTFORML (여기는……녀석의 방이군요)
					PRINTFORMW (너무 청소하고 싶지 않다……)
				CASEELSE
				;이것 정말로 괴로운, 열 39℃라든지 나오기 때문에, 1분 일어나기에 기침해 붐비기 때문에, 읽는 사람 있으면(자) 조심해
					PRINTFORML 「먼지에는 조심해주세요」
					PRINTFORMW 「너무 들이 마시면(자) 기관지가 염증을 일으킬테니까」
				ENDSELECT
			ENDSELECT
		ELSEIF TALENT:사모
			SELECTCASE RAND:4
			CASE 0
				PRINTFORML 「혼자서는 대단하겠지? 나도 도와요」
				PRINTFORMW 「……아아 됐어, 이렇게 먼지가」
			CASE 1
				PRINTFORML 「변함없이 더러워져」
				PRINTFORMW 「다시 수행이 필요할까……에서도……으응~」
			CASE 2
				PRINTFORML 「거처가 더러워져 있으면(자), 거기에 살고 있는 사람의 마음도 더러워집니다」
				PRINTFORMW 「그렇지만, 이 넓이를 매일 전부 청소하는 것은 분명히 무리일까……」
			CASEELSE
				SELECTCASE DAY:2
				CASE 1
					PRINTFORML 「밖의 세계에서는, 봄에 대청소를 하는 지역이 있다고 합니다」
					PRINTFORMW 「이라고 말하는 것으로, 확실히 청소할까요」
				CASE 2
					PRINTFORML 「후우……분명히 덥네요」
					PRINTFORML 카센은 촉촉하게 땀을 흘리고 있다
					PRINTFORMW %CALLNAME:MASTER%의 시선은 카센의 목덜미에 정부(제대로 고정) 가 되어 버렸다……
				CASE 3
					PRINTFORML 「등의 가을라는 말이 많습니다만, 저것은 가을의 날씨가 보내기 쉽기 때문에입니다」
					PRINTFORMW 「이라고 말하는 것으로, 확실히 청소할까요」
				CASE 4
					SELECTCASE DAY:3
					CASE 28
						PRINTFORML 「일년의 마지막의 대청소, 손은 빠지지 않아요」
						PRINTFORMW 「오늘은 진심을 보입니다」
						PRINTFORML 카센은 굉장한 속도로 움직이면서 청소를 하고 있는……이제 사람의 재주는 아니다
						PRINTFORMW 「이봐 %CALLNAME:MASTER%!손을 끊고 있는 짬은 없어요!」
					CASEELSE
						PRINTFORML 「우우……수가 차갑다……」
						PRINTFORML 카센은 손에 입김을 내뿜고 따뜻하게 하려 하고 있지만, 그다지 효과는 나와 있지 않은 것 같다
						PRINTFORMW 「녹초가 되면 안되구나, 청소하지 않으면」
					ENDSELECT
				ENDSELECT
			ENDSELECT
		ELSE
			IF TFLAG:193 == 1
				SELECTCASE RAND:3
				CASE 0
					PRINTFORMW 「혼자서는 큰 일이겠지요, 나도 돕습니다」
				CASE 1
					PRINTFORML 「%CALLNAME:MASTER%, 나에게도 빗자루와 쓰레받기를 부탁합니다」
					PRINTFORMW 「상관하지 않아요, 청소는 좋아하기 때문에」
				CASEELSE
					PRINTFORML 「변함없이……라고 말해입니다만, 더러워지네요」
					PRINTFORMW 「어쩔 수 없습니다, 나도 도웁시다」
				ENDSELECT
			ELSE
				SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「청소, 노력하네요」
					PRINTFORML 「……레이무는 세세한 곳까지는 청소하지 않지요」
					PRINTFORMW 「%마스터를% 봐 좋은 영향이 나오면 좋습니다만……」
				CASE 1
					SELECTCASE ABL:40
					CASE 0 TO 2
						PRINTFORML 「…………」
						PRINTFORML 카센은 %CALLNAME:MASTER%의 청소하는 모습을 가만히 응시하고 있다
						PRINTFORMW 아무래도 참견하고 싶어서 근질근질하고 있는 것 같다……
					CASEELSE
						PRINTFORML 「%마스터는% 청소를 좋아하는 것 같네요」
						PRINTFORMW 「손놀림이 익숙해져 있을테니까」
					ENDSELECT
				CASEELSE
					PRINTFORML 「그 옛날, 청소라는 것은 정신적인 이유가 강하고---」
					PRINTFORML 「서민의 사이에 퍼진 것은---」
